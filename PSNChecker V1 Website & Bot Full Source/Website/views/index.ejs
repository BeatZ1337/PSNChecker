            <div class="panel panel-default">
                <div class="panel-body">
                    <form id="psn" name="psn" action="/" method="POST">
                        <input type="hidden" name="_csrf" value="" />
                        <input type="hidden" name="_csrfToken" value="" />
                        <div id="result">
                            <center>
                                <% if (result==="AVAILABLE") { %>
                                <div class="alert alert-success" role="alert">
                                    <center><b><u>PSN Available</u><br> Congratulations the PSN <b><%= message %></b> is
                                            <code
                                                style="background-color: #222;"><b><font color="GREEN">available</font></b></code></b>.<br>Go
                                        Sign Up Here <a
                                            href="https://id.sonyentertainmentnetwork.com/create_account/#/create_account/wizard/entrance?entry=%2Fcreate_account">Playstation
                                            Network - Create Account</a></center>.
                                </div>
                                <% } else if (result==="TAKEN") { %>
                                <div class="alert alert-danger" role="alert">
                                    <center><b><u>PSN Taken</u></b><br>Sorry but the PSN <b><%= message %></b> is <code
                                            style="background-color: #222;"><b><font color="RED">taken</font></b></code>.
                                    </center>
                                </div>
                                <% } else if (result==="INTERNAL SERVER ERROR") { %>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>Error 500 - Internal Server Error</u><br><%= message %></b></center>.
                                </div>
                                <% } else if (result==="SECURITY CHECK ERROR") { %>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>SECURITY CHECK ERROR</u><br><%= message %></b></center>.
                                </div>
                                <% } else if (result==="PSN CONTAINS A NON-ALPHANUMERIC CHARACTER") { %>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>Non-Alphanumerical Character Error</u><br><%= message %></b></center>
                                    .<br><b>The only available characters allowed are: <br><code
                                            style="background-color: #222;"><b><font color="GREEN">a-z, A-Z, 0-9 and - _</font></b></code><br>Example:
                                        G0D0fW4R-_</b>
                                </div>
                                <% } else if (result==="4 LETTERS DON'T EXIST") { %>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>1-4 Letter Alphanumerical Character Error</u><br><%= message %></b>
                                    </center>.
                                </div>
                                <% } else if (result==="RATELIMITED") { %>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>Ratelimited</u><br><%= message %></b></center>.
                                </div>
                                <% } else if(result==="CAPTCHA") {%>
                                <div class="alert alert-warning" role="alert">
                                    <center><b><u>Please Sign In</u><br><%= message %></b></center>.
                                </div>
                                <% } else if(result==="PROFANITY") {%>
                                <div class="alert alert-danger" role="alert">
                                    <center><b><u>Profanity Detection</u></b><br>Sorry but the PSN <code
                                            style="background-color: #222;"><b><font color="RED"><%=message%></font></b></code>
                                        contains profanity which is not allowed on Sony Entertainment Network (PSN).
                                    </center>
                                </div>
                                <% } %>
                            </center>
                        </div>
